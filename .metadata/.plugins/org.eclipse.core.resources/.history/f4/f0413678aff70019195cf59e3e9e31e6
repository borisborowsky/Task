package com.company.app.data;

import java.util.Date;

public class BookPojoToJson {
	private String title;
	private String type;
	private String subject;
	private String authour;
	private Date publishDate;
	private double borrowPrice;
	private Date borrowDate;
	private Date returnDate;
	private String bookStatus;
	@Override
	public String toString() {
		return "{ \r\n" + 
				"	   \"title\":\"" + title + "\"" + ",\r\n" + 
				"	   \"type\":\"Crime\",\r\n" + 
				"	   \"subject\":\"for about subject\",\r\n" + 
				"	   \"authour\":\"Merena Bleyn\",\r\n" + 
				"	   \"publishDate\":\"2014-06-02\",\r\n" + 
				"	   \"borrowPrice\": \"2093\",\r\n" + 
				"	   \"borrowDate\": \"2014-06-02\",\r\n" + 
				"	   \"returnDate\": \"2014-06-02\",\r\n" + 
				"	   \"bookStatus\": \"TAKEN\"\r\n" + 
				"}";
	}
	
}

