package com.company.app;

public class MemberWrapperJSON {
	private String gender;
	private String name;
	private String phone;
	private String country;
	private String city;
	private String postCode;
	private String username;
	private String password;
	private String status;
	@Override
	public String toString() {
		return "{ \r\n" + 
				"   \"gender\":\"" + gender + "\",\r\n" + 
				"   \"name\":\"" + name + "\",\r\n" + 
				"   \"phone\":\"" + phone + "\",\r\n" + 
				"   \"address\":{ \r\n" + 
				"      \"country\":\"" + country + "\",\r\n" + 
				"      \"city\":\"Sofia\",\r\n" + 
				"      \"postCode\":\"1309\"\r\n" + 
				"   },\r\n" + 
				"   \"account\":{ \r\n" + 
				"      \"username\":\"gho5t\",\r\n" + 
				"      \"password\":\"280486\",\r\n" + 
				"      \"status\":\"ACTIVE\"\r\n" + 
				"   }\r\n" + 
				"}";
	}
	
	
	
	
}